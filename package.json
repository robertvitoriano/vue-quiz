{
  "name": "quiz-battle",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://quiz-battle.robertvitoriano.com/",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
